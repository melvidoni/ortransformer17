<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<GridPane maxHeight="200.0" maxWidth="500.0" minHeight="200.0" minWidth="500.0" prefHeight="200.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="gui.controllers.TransformController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>


  <Label text="Implementation:" GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM">
     <GridPane.margin><Insets bottom="5.0" right="10.0" /></GridPane.margin>
  </Label>
  <Label text="Database Type:" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
     <GridPane.margin><Insets right="10.0" /></GridPane.margin>
  </Label>
  <ComboBox fx:id="implementationCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.valignment="BOTTOM" />
  <ComboBox fx:id="dbtypeCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
  <Button mnemonicParsing="false" onAction="#cancelTransformation" text="Cancel" GridPane.columnIndex="1"
          GridPane.halignment="RIGHT" GridPane.rowIndex="2">
     <GridPane.margin><Insets right="50.0" /></GridPane.margin>
  </Button>
  <Button mnemonicParsing="false" onAction="#startTransformation" text="Transform" GridPane.columnIndex="1"
          GridPane.halignment="CENTER" GridPane.rowIndex="2" />

</GridPane>
